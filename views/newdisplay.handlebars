						<label for="theme-light" class="container-modal-theme-content-item item-active" data-theme="light">
							<img src="https://cdn.glitch.com/bb41c722-2b89-4fa0-ac62-0d43042ec695%2Ftheme_light.png" alt="">
							<div class="container-modal-theme-content-item-choose">
								<input type="radio" id="theme-light" name="theme" checked>
								<div>Sáng</div>
							</div>
						</label>
						<label for="theme-dark" class="container-modal-theme-content-item" data-theme="dark">
							<img src="https://cdn.glitch.com/bb41c722-2b89-4fa0-ac62-0d43042ec695%2Ftheme-dark.png" alt="">
							<div class="container-modal-theme-content-item-choose">
								<input type="radio" id="theme-dark" name="theme">
								<div>Tối</div>
							</div>
						</label>
						<label for="theme-dark-dimmed" class="container-modal-theme-content-item" data-theme="dark-dimmed">
							<img src="https://cdn.glitch.com/bb41c722-2b89-4fa0-ac62-0d43042ec695%2Ftheme-dark-dimmed.png" alt="">
							<div class="container-modal-theme-content-item-choose">
								<input type="radio" id="theme-dark-dimmed" name="theme">
								<div>Tối mờ</div>
							</div>
						</label>
					</div>
					<div class="container-modal-theme-handle">
						<button class="btn btn-back menu-item-theme-back">Trở về</button>
						<button class="btn btn-save menu-item-theme-save">Lưu lại</button>
					</div>
				</div>
			</div>
		</div>
	</div>
	<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
	<script>
		$('.container-modal-theme-content-item').on('click', function() {
			var theme = $(this).attr('data-theme')
			$(this).siblings().removeClass('item-active')
			$(this).addClass('item-active')
			$('html').attr('data-theme', theme)
		})
		$(document).on('click', '.menu-item-theme', function() {
			$('.container-modal').addClass('modal-theme-active')
		}).on('click', '.menu-item-theme-save', function() {
			$('.container-modal').removeClass('modal-theme-active')
		}).on('click', '.menu-item-theme-back', function() {
			$('.container-modal').removeClass('modal-theme-active')
		})
		$('.menu-item-search').on('click', function() {
			$('.header-nav-modal-search').toggleClass('modal-search-active')
		})
		$('.menu-item-toggle').on('click', function() {
			$('.container').toggleClass('container-side-active')
		})
		$('.post-action-reaction').on('click', function() {
			$(this).toggleClass('reaction-active')
		})
		$('.post-action-comment').on('click', function() {
			$(this).parents('.post').find('.post-comment').toggleClass('post-comment-active')
		})
	</script>
</body>
</html>